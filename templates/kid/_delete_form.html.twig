{% include 'blocks/success.html.twig' %}
<form method="post" action="{{ path('app_kid_delete', {'id': kid.id}) }}" onsubmit="return confirm('{{ 'confirm.delete'|trans }}');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ kid.id) }}">
    <button class="btn btn-info btn-block">{{ 'button.delete'|trans }}</button>
</form>
